{
  "name": "kirian-caumes-website",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "20.x.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "npx next-sitemap",
    "start": "next start",
    "eslint:fix": "eslint src --ext .tsx,.ts,.js,.jsx --fix",
    "eslint": "eslint src --ext .tsx,.ts,.js,.jsx",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,sass}\" --fix;",
    "stylelint": "stylelint \"src/**/*.{css,scss,sass}\"",
    "prettier:fix": "npx prettier --write .",
    "prettier": "npx prettier --check .",
    "cspell": "npx cspell ./ --no-progress --dot --gitignore",
    "typed-scss-modules": "npx typed-scss-modules src --nameFormat none --exportType default --banner \"/* eslint-disable jsdoc/require-jsdoc, @typescript-eslint/consistent-type-definitions */\" --logLevel info --exportTypeName ClassNames --exportTypeInterface Styles",
    "prepare": "husky install",
    "knip": "NODE_OPTIONS=\"--max-old-space-size=8192\" npx knip"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "js-cookie": "^3.0.5",
    "next": "14.0.3",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-awesome-reveal": "^4.2.7",
    "react-dom": "18.2.0",
    "react-ga4": "^2.1.0",
    "sass": "^1.69.5",
    "sharp": "^0.33.0",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@cspell/dict-fr-fr": "^2.2.2",
    "@next/eslint-plugin-next": "^14.0.3",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^20.10.4",
    "@types/react": "18.2.42",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "cspell": "^8.1.3",
    "eslint": "^8.55.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "14.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "knip": "^3.6.0",
    "lint-staged": "^15.2.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.32",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.1.0",
    "stylelint": "^15.11.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typed-scss-modules": "^8.0.0"
  }
}
