{
  "name": "kirian-caumes-website",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "engines": {
    "node": "18.x.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "npx next-sitemap",
    "start": "next start",
    "eslint:fix": "eslint src --ext .tsx,.ts,.js,.jsx --fix",
    "eslint": "eslint src --ext .tsx,.ts,.js,.jsx",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss,sass}\" --fix;",
    "stylelint": "stylelint \"src/**/*.{css,scss,sass}\"",
    "prettier:fix": "npx prettier --write .",
    "prettier": "npx prettier --check .",
    "cspell": "npx cspell ./ --no-progress --dot --gitignore",
    "typed-scss-modules": "npx typed-scss-modules src --nameFormat none --exportType default --banner \"/* eslint-disable jsdoc/require-jsdoc, @typescript-eslint/consistent-type-definitions */\" --logLevel info --exportTypeName ClassNames --exportTypeInterface Styles",
    "prepare": "husky install"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "js-cookie": "^3.0.5",
    "next": "13.5.6",
    "next-pwa": "^5.6.0",
    "react": "18.2.0",
    "react-awesome-reveal": "^4.2.5",
    "react-dom": "18.2.0",
    "react-ga4": "^2.1.0",
    "sass": "^1.69.4",
    "sharp": "^0.32.6",
    "typescript": "5.2.2"
  },
  "devDependencies": {
    "@cspell/dict-fr-fr": "^2.2.2",
    "@next/eslint-plugin-next": "^13.5.6",
    "@types/js-cookie": "^3.0.5",
    "@types/node": "^18.18.6",
    "@types/react": "18.2.31",
    "@types/react-dom": "18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "cspell": "^7.3.8",
    "eslint": "^8.52.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "13.5.6",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.8.2",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.0.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.31",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "stylelint": "^15.11.0",
    "stylelint-config-sass-guidelines": "^10.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typed-scss-modules": "^7.1.4"
  }
}
